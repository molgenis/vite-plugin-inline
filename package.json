{
  "name": "@molgenis/vite-plugin-inline",
  "version": "1.0.0",
  "description": "Vite plugin that embeds .js and .css as base85 data ",
  "keywords": ["vite", "vite-plugin"],
  "scripts": {
    "format": "prettier --write src/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/molgenis/vite-plugin-inline.git"
  },
  "license": "LGPL-3.0",
  "lint-staged": {
    "src/**/*.ts": [
      "eslint",
      "prettier --write"
    ]
  },
  "bugs": {
    "url": "https://github.com/molgenis/vite-plugin-inline/issues"
  },
  "homepage": "https://github.com/molgenis/vite-plugin-inline#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "c8": "^7.11.3",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vitest": "^0.12.6"
  },
  "dependencies": {
    "fflate": "^0.7.3"
  }
}
