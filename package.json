{
  "name": "@molgenis/vite-plugin-inline",
  "version": "1.0.7",
  "description": "Vite plugin that embeds .js and .css as base85 data ",
  "keywords": [
    "vite",
    "vite-plugin"
  ],
  "scripts": {
    "prepack": "tsc",
    "format": "prettier --write src/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "test": "vitest run --config vitest.config.cts",
    "test:watch": "vitest --config vitest.config.cts",
    "coverage": "vitest --config vitest.config.cts run --coverage",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/molgenis/vite-plugin-inline.git"
  },
  "license": "LGPL-3.0",
  "lint-staged": {
    "src/**/*.ts": [
      "eslint",
      "prettier --write"
    ]
  },
  "bugs": {
    "url": "https://github.com/molgenis/vite-plugin-inline/issues"
  },
  "homepage": "https://github.com/molgenis/vite-plugin-inline#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^17.0.42",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "c8": "^7.11.3",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "prettier": "^2.7.0",
    "typescript": "^4.7.3",
    "vite": "^2.9.12",
    "vitest": "^0.14.2"
  },
  "dependencies": {
    "fflate": "^0.7.3"
  }
}
