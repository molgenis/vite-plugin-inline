{
  "name": "@molgenis/vite-plugin-inline",
  "version": "2.0.0",
  "description": "Vite plugin that embeds .js and .css as base85 data ",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/plugin/index.d.ts",
      "import": "./dist/plugin/index.mjs"
    },
    "./loader": "./dist/loader/loader.js"
  },
  "module": "./dist/plugin/index.mjs",
  "types": "./dist/plugin/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "vite",
    "vite-plugin"
  ],
  "scripts": {
    "generate": "node scripts/generate_wasm_wrappers.mjs",
    "prebuild": "pnpm run generate && tsc",
    "build": "node build-plugin.mjs && node build-loader.mjs",
    "prepack": "pnpm build",
    "format": "prettier --write src/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "pretest": "pnpm run generate && tsc",
    "test": "vitest run --config vitest.config.mts",
    "test:watch": "vitest --config vitest.config.mts",
    "coverage": "vitest --config vitest.config.mts run --coverage",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/molgenis/vite-plugin-inline.git"
  },
  "license": "LGPL-3.0",
  "lint-staged": {
    "src/**/*.ts": [
      "eslint",
      "prettier --write"
    ]
  },
  "bugs": {
    "url": "https://github.com/molgenis/vite-plugin-inline/issues"
  },
  "homepage": "https://github.com/molgenis/vite-plugin-inline#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.2",
    "@vitest/coverage-v8": "^4.0.15",
    "esbuild": "^0.27.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^9.1.2",
    "husky": "^9.1.7",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0",
    "vitest": "^4.0.15"
  },
  "peerDependencies": {
    "vite": "^6.4.1"
  },
  "engines": {
    "node": "22",
    "pnpm": "9"
  }
}
